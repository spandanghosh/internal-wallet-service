# =============================================================================
# Internal Wallet Service - Environment Variables
# Copy this file to .env and fill in your values for local development.
# (docker-compose.yml sets these automatically for the Docker environment)
# =============================================================================

# PostgreSQL connection string
# Format: jdbc:postgresql://<host>:<port>/<database>
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/wallet_db
SPRING_DATASOURCE_USERNAME=wallet
SPRING_DATASOURCE_PASSWORD=wallet_secret

# HTTP port the service listens on (default: 8080)
SERVER_PORT=8080
